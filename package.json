{
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "smol-toml": "^1.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^20.0.0",
    "@vercel/ncc": "^0.38.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "name": "github-metrics",
  "private": true,
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "fmt": "biome format src/",
    "fmt:fix": "biome check --fix src/",
    "generate": "tsx src/index.ts",
    "lint": "biome check src/",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "version": "0.4.6"
}

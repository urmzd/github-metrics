{
  "name": "github-metrics",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "fmt": "prettier --check 'src/**/*.{ts,tsx}'",
    "fmt:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "generate": "tsx src/index.ts"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^9.0.0",
    "prettier": "^3.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^2.0.0"
  }
}
